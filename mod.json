{
	"geode": "4.2.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "ninxout.prntscrn",
	"name": "PRNTSCRN",
	"version": "v1.0.0",
	"developers": ["ninXout", "mat"],
	"description": "Quick and easy screenshot tool!",
	"resources": {
        "sprites": [
            "resources/screenshot.png"
        ]
    },
	"settings": {
		"copy-clipboard": {
			"type": "bool",
			"name": "Copy to Clipboard",
			"default": true
		},
		"hide-player": {
			"type": "bool",
			"name": "Hide Player",
			"default": false
		},
		"hide-ui": {
			"type": "bool",
			"name": "Hide UI",
			"default": true
		},
		"resolution-width": {
			"type": "int",
			"name": "Resolution Width",
			"description": "The width of the image",
			"default": 1920
		},
		"resolution-height": {
			"type": "int",
			"name": "Resolution Height",
			"description": "The height of the image",
			"default": 1080
		},
		"jpeg-mafia": {
			"type": "bool",
			"name": "Output to JPEG",
			"description": "Use JPEG for images instead of PNG",
			"default": false
		},
		"auto-screenshot": {
			"type": "bool",
			"name": "Auto Screenshot",
			"description": "Automatically at every percentage interval",
			"default": false
		},
		"auto-percent": {
			"type": "int",
			"name": "Auto Screenshot Percentage",
			"description": "Automatically at every percentage interval",
			"default": 10
		}
	}
}
